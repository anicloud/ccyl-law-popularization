<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ani.ccyl.leg.persistence.mapper.SignInMapper">
    <select id="findByConditions" parameterType="com.ani.ccyl.leg.persistence.po.SignInPO" resultType="com.ani.ccyl.leg.persistence.po.SignInPO">
        SELECT * FROM t_sign_in WHERE account_id=#{accountId} AND date_format(#{createTime},'%Y-%m-%d')=date_format(create_time,'%Y-%m-%d') AND is_del=0
    </select>
</mapper>